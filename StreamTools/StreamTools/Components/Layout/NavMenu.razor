        <BarItem>
            <BarLink To="">
                Stream Tools <BarIcon IconName="IconName.Tablet" />
            </BarLink>
        </BarItem>
    </BarBrand>
    <BarToggler />
    <BarMenu>
        <BarStart>
            <BarItem>
                <BarLink To="" Match="Match.All">
                    Home <BarIcon IconName="IconName.Dashboard"/>
                </BarLink>
            </BarItem>
            <BarItem>
                <BarLink To="twitch" Match="Match.All">
                    Twitch <BarIcon IconName="IconName.Camera"/>
                </BarLink>
            </BarItem>
            <BarItem>
                <BarLink To="youtube" Match="Match.All">
                    Youtube <BarIcon IconName="IconName.Map" />
                </BarLink>
            </BarItem>
        </BarStart>
    </BarMenu>
</Bar>
